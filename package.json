{
  "name": "hrafnkellbaldurs.com",
  "description": "Hrafnkell Baldursson Portfolio page",
  "author": "Hrafnkell Baldursson",
  "license": "MIT",
  "scripts": {
    "prod": "gatsby build",
    "prod:verbose": "gatsby build --no-uglify --verbose",
    "prod:serve": "npm run prod && npm run serve",
    "serve": "gatsby serve --port 8080",
    "dev": "gatsby develop",
    "format": "eslint ./src/ --ext .js,.jsx --fix",
    "test": "jest --json --outputFile=jest-test-results.json",
    "test:watch": "jest --watch",
    "test:coverage": "npm run test -- --coverage",
    "test:nooutput": "jest",
    "storybook": "start-storybook -p 6006",
    "storybook:prod": "build-storybook -o docs"
  },
  "keywords": [
    "gatsby",
    "storybook",
    "jest"
  ],
  "dependencies": {
    "@svgr/webpack": "^4.0.3",
    "classnames": "^2.2.6",
    "gatsby": "^2.0.33",
    "gatsby-plugin-manifest": "^2.0.9",
    "gatsby-plugin-react-helmet": "^3.0.2",
    "gatsby-plugin-sass": "^2.0.4",
    "gatsby-plugin-svgr": "^2.0.0-alpha",
    "gatsby-source-filesystem": "^2.0.8",
    "gatsby-transformer-json": "^2.1.5",
    "node-sass": "^4.10.0",
    "ramda": "^0.26.0",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-fittext": "^1.0.0",
    "react-helmet": "^5.2.0",
    "smooth-scroll-into-view-if-needed": "^1.1.23"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@storybook/addon-actions": "^4.0.9",
    "@storybook/addon-backgrounds": "^4.0.8",
    "@storybook/addon-centered": "^4.0.8",
    "@storybook/addon-jest": "^4.0.9",
    "@storybook/addon-knobs": "^4.0.9",
    "@storybook/addon-links": "^4.0.9",
    "@storybook/addon-options": "^4.0.8",
    "@storybook/addon-storysource": "^4.0.9",
    "@storybook/addon-viewport": "^4.0.9",
    "@storybook/addons": "^4.0.9",
    "@storybook/react": "^4.0.9",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-preset-gatsby": "^0.1.4",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.8.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.1.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^4.0.0",
    "identity-obj-proxy": "^3.0.0",
    "include-media": "^1.4.9",
    "jest": "^23.6.0",
    "react-test-renderer": "^16.6.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hrafnkellbaldurs/hrafnkellbaldurs-netlify"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/jest-preprocess.js"
    },
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss)$": "identity-obj-proxy",
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "setupTestFrameworkScriptFile": "./setupTests.js",
    "testPathIgnorePatterns": [
      "node_modules",
      "/.cache/"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)"
    ],
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "setupFiles": [
      "<rootDir>/loadershim.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
